.text
  flex($justify: center)
  width 100%
  height 100%
  padding 10%
  background-color white
  border 2px solid $colorBlack
  border-radius 100%

  /.toolbox &
    max-width 80%
    justify-content flex-start
    white-space nowrap
    font-size 12px
    overflow hidden
    text-overflow ellipsis
